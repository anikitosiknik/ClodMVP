{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "globals": {
        "JSX": true,
        "Partial": true,
        "RequestInfo": true,
        "RequestInit": true,
        "ReadableStreamDefaultReader": true,
        "RegExpExecArray": true
    }
}
